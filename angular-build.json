{
  "$schema": "./node_modules/@dagonmetric/angular-build/src/schemas/schema.json#",
  "libs": [
    {
      "extends": "ngb:lib-default",
      "name": "ng-log",
      "root": "modules/ng-log",
      "outputPath": "dist/packages/ng-log",
      "libraryName": "ngLog",
      "envOverrides": {
        "prod": {
          "copy": [
            "README.md",
            "../../LICENSE"
          ],
          "bundles": true,
          "packageJsonCopy": true,
          "banner": "../banner.txt"
        }
      }
    },
    {
      "extends": "ngb:lib-default",
      "name": "ng-log-console",
      "root": "modules/ng-log/console",
      "outputPath": "dist/packages/ng-log",
      "libraryName": "ngLog.console",
      "externals": {
        "@dagonmetric/ng-log": "ngLog"
      },
      "envOverrides": {
        "prod": {
          "copy": [
            {
              "from": "README.md",
              "to": "console/README.md"
            }
          ],
          "bundles": true,
          "packageJsonCopy": true
        }
      }
    },
    {
      "extends": "ngb:lib-default",
      "name": "ng-log-config",
      "root": "modules/ng-log/config",
      "outputPath": "dist/packages/ng-log",
      "libraryName": "ngLog.config",
      "externals": {
        "@dagonmetric/ng-config": "ngConfig",
        "@dagonmetric/ng-log": "ngLog"
      },
      "envOverrides": {
        "prod": {
          "copy": [
            {
              "from": "README.md",
              "to": "console/README.md"
            }
          ],
          "bundles": true,
          "packageJsonCopy": true
        }
      }
    }
  ],
  "apps": []
}
